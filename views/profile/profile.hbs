<section id="profile-header">
  <h1>{{ profile.username }}</h1>
  <img src="{{profile.picture}}" alt="User's profile picture" id="image-profile">
  <p>{{profile.bio}}</p>

{{#if isOwnProfile}}
  <a href="/profile/edit">Edit Profile</a>
{{/if}}

</section>


<div id="profile-card-container">
{{#each paths}}
{{>routecard}}
{{/each}}
</div>



